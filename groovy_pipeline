pipeline
{
    agent none 

    stages    
    {
        stage ('stge-1')
        { steps       
          {
              sh 'echo Hi, this is Jenkins'
              ksh 'echo Hi, this is ksh file'
              
          }
        }  
        stage ('approval from QA')
        { steps       
          {
              input ('Please approve testing')
              
          }
        }  
        stage ('stge-2')
        { steps
         {
             sh 'echo Hi, this is STGE-2'
         }

        } 
    }
}
